# ai_agent/.env.example - Environment variables template (no secrets)

# Tavily API Key (get from https://tavily.com)
TAVILY_API_KEY=your_tavily_api_key_here

# OpenAI API Key (get from https://platform.openai.com)
OPENAI_API_KEY=your_openai_api_key_here

# FastAPI Configuration
APP_HOST=0.0.0.0
APP_PORT=8000

# Database (if needed)
DATABASE_URL=mysql://airbnb:password@localhost:3306/airbnb

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:4000

# LangChain Tracing/Endpoint (if used)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_PROJECT=ai-concierge-agent
